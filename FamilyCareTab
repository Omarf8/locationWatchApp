import SwiftUI

struct FamilyCareTab: View {
    var body: some View {
        ZStack {
            Color(.white)
            
            ScrollView {
                Image("dogImage")
                    .resizable()
                    .aspectRatio(contentMode: .fit)
                    .edgesIgnoringSafeArea(.top)
                
                
            } // ScrollView
            .edgesIgnoringSafeArea(.top)
            
        } // ZStack
        .navigationBarBackButtonHidden(true)
        
    }
}

#Preview {
    FamilyCareTab()
}
